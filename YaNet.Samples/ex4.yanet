Host: easypay.ua

Requests:

	GooglGet: <GetRequest>
		Method: GET
		Url: https://google.com/
		Headers:
			Host: google.com
			UserAgent: Mozilla/5.0
			Referrer: https://google.com/

	GooglePost: <GetRequest>
		Method: POST
		Url: https://google.com/
		Headers:
			Host: google.com
			UserAgent: Mozilla/5.0
			Referrer: https://google.com/
			Origin: https://google.com
		MimeType: application/x-www-form-urlencoded; charset=UTF-8
		Body:
			User:
				Name: John
				Age: 25
